{
  "name": "alphabet-filter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "heroku-postbuild": "ng build --aot --prod",
    "build:lib": "rimraf dist && npm run packagr && node copy && cd dist && npm pack",
    "publish-pkg": "pushd dist && npm publish",
    "build-and-publish": "npm run build:lib && npm run publish-pkg",
    "packagr": "./node_modules/.bin/ng-packagr -p alphabet-filter/package.json",
    "deploy-git-pages": "ng build --aot --prod --base-href=/alphabet-filter/ && node node_modules/gh-pages/bin/gh-pages -d dist/alphabet-filter"
  },
  "engines": {
    "node": "12.14.0",
    "npm": "6.13.4"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.1.3",
    "@angular/common": "^10.1.3",
    "@angular/compiler": "^10.1.3",
    "@angular/core": "^10.1.3",
    "@angular/forms": "^10.1.3",
    "@angular/platform-browser": "^10.1.3",
    "@angular/platform-browser-dynamic": "^10.1.3",
    "@angular/router": "^10.1.3",
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "fast-deep-equal": "^3.1.1",
    "gh-pages": "^2.2.0",
    "path": "^0.12.7",
    "rxjs": "^6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.3",
    "@angular/cli": "~10.1.3",
    "@angular/compiler-cli": "^10.1.3",
    "@angular/language-service": "^10.1.3",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.0.5",
    "codelyzer": "^5.1.2",
    "cpx": "^1.5.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.0",
    "node-sass": "^4.14.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.10.1",
    "tslint": "~6.1.0",
    "typescript": "~4.0.3"
  }
}
